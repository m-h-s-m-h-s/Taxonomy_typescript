{
  "name": "typescript-taxonomy",
  "version": "1.0.0",
  "description": "TypeScript implementation of AI-powered product taxonomy classification using Google's product categories and OpenAI GPT models",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-h-s-m-h-s/Taxonomy_typescript.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/examples/usage-examples.js",
    "check": "node scripts/check-setup.js",
    "test": "jest",
    "interactive": "node dist/interactiveInterface.js",
    "batch-test": "node dist/simpleBatchTester.js",
    "classify": "node scripts/classify-single-product.js",
    "test-random": "node scripts/test-random-products.js"
  },
  "bin": {
    "taxonomy-interactive": "./dist/interactiveInterface.js",
    "taxonomy-test": "./dist/simpleBatchTester.js",
    "taxonomy-classify": "./scripts/classify-single-product.js"
  },
  "keywords": [
    "taxonomy", 
    "classification", 
    "openai", 
    "gpt", 
    "product-categorization",
    "google-taxonomy",
    "ai",
    "machine-learning",
    "e-commerce"
  ],
  "author": "MHS",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.0.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
